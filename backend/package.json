{
  "name": "meleeme-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "dev:local": "NODE_ENV=local npm run dev",
    "dev:docker:start": "docker-compose -f docker-compose.dev.yaml up",
    "dev:docker:stop": "docker-compose -f docker-compose.dev.yaml down",
    "build": "tsc",
    "orm:cli": "ts-node ./node_modules/.bin/typeorm -f ./ormconfig.json",
    "test": "jest --watch",
    "prod:start": "NODE_ENV=production NODE_PATH=.:$(npm root --quiet -g) pm2 start ./build/index.js --name \"web-server\"",
    "prod:test": "NODE_ENV=development NODE_PATH=.:$(npm root --quiet -g) pm2 start ./build/index.js --name \"web-server\"",
    "prod:stop": "pm2 stop web-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cavisd7/MeleeMe.git"
  },
  "author": "Casey Davis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cavisd7/MeleeMe/issues"
  },
  "homepage": "https://github.com/cavisd7/MeleeMe#readme",
  "dependencies": {
    "@slippi/slippi-js": "5.0.5",
    "aws-sdk": "^2.788.0",
    "bcrypt": "^5.0.0",
    "busboy": "^0.3.1",
    "connect-redis": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "helmet": "^4.1.0",
    "ioredis": "^4.17.3",
    "jsonschema": "^1.2.6",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.25",
    "uuid": "^8.3.1",
    "validate.js": "^0.13.1",
    "winston": "^3.3.3",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/bcrypt": "^3.0.0",
    "@types/busboy": "^0.2.3",
    "@types/cookie": "^0.4.0",
    "@types/express": "4.17.7",
    "@types/express-session": "^1.17.3",
    "@types/ioredis": "^4.17.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^8.0.3",
    "@types/validate.js": "^0.11.0",
    "@types/ws": "^7.2.6",
    "chai": "^4.2.0",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
